<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

  <Button
      android:id="@+id/open_string"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:layout_margin="@dimen/small_margin"
      android:text="@string/open_string"
      app:layout_constraintBottom_toTopOf="@+id/open_bytes"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"

      app:layout_constraintVertical_chainStyle="packed"
      />

  <Button
      android:id="@+id/open_bytes"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:layout_margin="@dimen/small_margin"
      android:text="@string/open_bytes"
      app:layout_constraintBottom_toTopOf="@+id/open_save"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/open_string"
      />

  <Button
      android:id="@+id/open_save"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:layout_margin="@dimen/small_margin"
      android:text="@string/open_save"
      app:layout_constraintBottom_toTopOf="@+id/list_all"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/open_bytes"
      />

  <Button
      android:id="@+id/list_all"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:layout_margin="@dimen/small_margin"
      android:text="@string/list_all"
      app:layout_constraintBottom_toTopOf="@+id/list_open"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/open_save"
      />

  <Button
      android:id="@+id/list_open"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:layout_margin="@dimen/small_margin"
      android:text="@string/list_open"
      app:layout_constraintBottom_toTopOf="@+id/list_open_string"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/list_all"
      />

  <Button
      android:id="@+id/list_open_string"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:layout_margin="@dimen/small_margin"
      android:text="@string/list_open_string"
      app:layout_constraintBottom_toTopOf="@+id/list_open_bytes"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/list_open"
      />

  <Button
      android:id="@+id/list_open_bytes"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:layout_margin="@dimen/small_margin"
      android:text="@string/list_open_bytes"
      app:layout_constraintBottom_toTopOf="@+id/list_open_save"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/list_open_string"
      />

  <Button
      android:id="@+id/list_open_save"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:layout_margin="@dimen/small_margin"
      android:text="@string/list_open_save"
      app:layout_constraintBottom_toTopOf="@+id/list_open_fd"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/list_open_bytes"
      />

  <Button
      android:id="@+id/list_open_fd"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:layout_margin="@dimen/small_margin"
      android:text="@string/list_open_fd"
      app:layout_constraintBottom_toTopOf="@+id/list_open_non_asset_fd"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/list_open_save"
      />

  <Button
      android:id="@+id/list_open_non_asset_fd"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:layout_margin="@dimen/small_margin"
      android:text="@string/list_open_non_asset_fd"
      app:layout_constraintBottom_toTopOf="@+id/list_open_xml_resource_parser"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/list_open_fd"
      />

  <Button
      android:id="@+id/list_open_xml_resource_parser"
      android:layout_width="0dp"
      android:layout_height="0dp"
      android:layout_margin="@dimen/small_margin"
      android:text="@string/list_open_xml_resource_parser"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toBottomOf="@+id/list_open_non_asset_fd"
      />

</android.support.constraint.ConstraintLayout>